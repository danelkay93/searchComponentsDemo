{
    "downshift": {
        "stateReducer": "(state, changes) => ({ ...changes, isOpen: false })",
        "selectedItem": null,
        "itemToString": "(item) => item || ''",
        "onSelectedItemChange": "(changes) => console.log('selected item changed', changes.selectedItem)"
    },
    "multipleSelection": {
        "initialSelectedItems": [],
        "itemToString": "(item) => item || ''",
        "onSelectedItemsChange": "(changes) => console.log('selected items changed', changes.selectedItems)"
    },
    "materialUiChip": {
        "variant": "outlined",
        "size": "small",
        "onDelete": true,
        "className": "filter-chip"
    },
    "reactIcons": {
        "searchIcon": {
            "size": 16,
            "className": "search-icon"
        },
        "arrowIcons": {
            "size": 12,
            "className": "suggestion-arrow"
        }
    },
    "inlineSuggestion": {
        "className": "inline-suggestion",
        "fadedTextClass": "faded-suggestion"
    }
}
