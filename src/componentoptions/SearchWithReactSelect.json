{
    "reactSelect": {
        "isMulti": true,
        "isClearable": true,
        "isSearchable": true,
        "placeholder": "Search cards...",
        "noOptionsMessage": "No matching cards found",
        "components": {
            "IndicatorSeparator": null,
            "DropdownIndicator": null
        },
        "styles": {
            "control": "(base) => ({ ...base, borderRadius: '4px' })",
            "multiValue": "(base) => ({ ...base, backgroundColor: 'transparent' })"
        }
    },
    "materialUiChip": {
        "variant": "outlined",
        "size": "small",
        "onDelete": true,
        "className": "filter-chip"
    },
    "inlineSuggestion": {
        "className": "inline-suggestion",
        "fadedTextClass": "faded-suggestion",
        "arrowClass": "suggestion-arrow"
    }
}
